{
  "name": "mockgoose",
  "description": "Mockgoose is an in memory database mock to allow for testing of applications that rely on Mongoose.",
  "version": "6.0.9",
  "author": {
    "name": "Anthony McCormick",
    "email": "anthony.mccormick AT gmail.com"
  },
  "keywords": [
    "mongodb",
    "mongoose",
    "datastore",
    "nosql",
    "sql",
    "db",
    "database",
    "mock",
    "stub",
    "in memory"
  ],
  "scripts": {
    "hint": "jshint --config .jshintrc Mockgoose.js test",
    "test": "DEBUG=mongodb*,Mockgoose mocha --timeout 50000 test",
    "test-debug": "DEBUG=mongodb*,Mockgoose mocha debug --timeout 50000 test"
  },
  "main": "Mockgoose",
  "files": [
    "Mockgoose.js",
    "lib/"
  ],
  "engines": {
    "node": ">=0.10.0"
  },
  "dependencies": {
    "debug": "2.2.0",
    "mongodb-prebuilt": "git+https://github.com/preempt/mongodb-prebuilt.git",
    "portfinder": "^1.0.3",
    "q": "^1.4.1",
    "rimraf": "^2.5.1"
  },
  "devDependencies": {
    "chai": "~2.2.0",
    "mocha": "~2.2.4",
    "mongoose": "*"
  },
  "bugs": {
    "url": "https://github.com/mccormicka/Mockgoose/issues"
  },
  "repository": "Mockgoose/Mockgoose",
  "homepage": "https://github.com/Mockgoose/Mockgoose",
  "readmeFilename": "README.md",
  "license": "MIT"
}
